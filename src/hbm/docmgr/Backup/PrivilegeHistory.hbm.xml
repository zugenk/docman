<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.app.docmgr.model">
	<class
		name="PrivilegeHistory"
		table="privilege_history"
	>
		<id
			name="id"
			type="java.lang.Long"
			column="id"
		>
			<generator class="sequence">
				<param name="sequence">privilege_history_id_seq</param>
            </generator>
		</id>
		<property
			name="historyDate"
			column="history_date"
			type="java.util.Date"
			not-null="true"
		/>
		<property
			name="historyBy"
			column="history_by"
			type="string"
			not-null="true"
			length="20"
		/>
		<property
			name="auditTrailId"
			column="audit_trail_id"
			type="long"
			not-null="true"
		/>
		
		<property
			name="privilegeId"
			column="privilege_id"
			type="long"
			not-null="true"
		/>

		<property
			name="name"
			column="name"
			type="string"
			not-null="true"
			length="100"
		/>
		<property
			name="vgroup"
			column="vgroup"
			type="string"
			not-null="true"
			length="50"
		/>
		<property
			name="description"
			column="description"
			type="string"
			not-null="false"
			length="100"
		/>

	</class>	
</hibernate-mapping>
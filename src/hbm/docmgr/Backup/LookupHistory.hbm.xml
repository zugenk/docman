<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.app.docmgr.model">
	<class
		name="LookupHistory"
		table="lookup_history"
	>
		<id
			name="id"
			type="java.lang.Long"
			column="id"
		>
			<generator class="sequence">
				<param name="sequence">lookup_history_id_seq</param>
            </generator>
		</id>
		<property
			name="historyDate"
			column="history_date"
			type="java.util.Date"
			not-null="true"
		/>
		<property
			name="historyBy"
			column="history_by"
			type="string"
			not-null="true"
			length="20"
		/>
		<property
			name="auditTrailId"
			column="audit_trail_id"
			type="long"
			not-null="true"
		/>
		<property
			name="lookupId"
			column="lookup_id"
			type="java.lang.Long"
			not-null="true"
		/>
		<property
			name="type"
			column="type"
			type="string"
			not-null="true"
			length="20"
		/>
		<property
			name="code"
			column="code"
			type="string"
			not-null="true"
			length="20"
		/>
		<property
			name="name"
			column="name"
			type="string"
			not-null="true"
			length="20"
		/>
		<property
			name="priority"
			column="priority"
			type="integer"
			not-null="false"
		/>
		<property
			name="description"
			column="description"
			type="string"
			not-null="false"
			length="100"
		/>
		<property
			name="shortname"
			column="shortname"
			type="string"
			not-null="false"
			length="10"
		/>
		<many-to-one
			name="status"
			column="status"
			class="com.app.docmgr.model.Status"
			not-null="true"
		>
		</many-to-one>
		
	</class>	
</hibernate-mapping>
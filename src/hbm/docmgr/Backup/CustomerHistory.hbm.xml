		/>
		<set name="users" inverse="true">
			<key column="customer_id"/>
			<one-to-many class="com.app.docmgr.model.User"/>
		</set>
		<set name="accounts" inverse="true">
			<key column="customer_id"/>
			<one-to-many class="com.app.docmgr.model.Account"/>
		</set>

		<set name="targetAccounts" inverse="true">
			<key column="customer_id"/>
			<one-to-many class="com.app.docmgr.model.TargetAccount"/>
		</set>

		<set
			name="roles"
			table="customer_history_role"
			cascade="all-delete-orphan"
		>
			<key column="customer_history_id"/>
			<many-to-many column="role_id" class="com.app.docmgr.model.Role"/>
		</set>

	</class>
</hibernate-mapping>

<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.app.docmgr.model">
	<class
		name="TargetAccount"
		table="target_account"
	>
		<id
			name="id"
			type="java.lang.Long"
			column="id"
		>
			<generator class="sequence">
				<param name="sequence">target_account_id_seq</param>
            </generator>
		</id>
		<property
			name="name"
			column="name"
			type="string"
			not-null="false"
			length="30"
		/>
		<property
			name="accountType"
			column="account_type"
				type="string"
			not-null="true"
			length="50"
		/>
		<property
			name="accountNumber"
			column="account_number"
			type="string"
			not-null="false"
			length="30"
		/>
		<property
			name="bank"
			column="bank"
			type="string"
			not-null="true"
			length="30"
		/>
		<property
			name="bankBranch"
			column="bank_branch"
			type="string"
			not-null="false"
			length="30"
		/>
		<property
			name="description"
			column="description"
			type="string"
			not-null="false"
			length="100"
		/>
		<property
			name="currency"
			column="currency"
			type="string"
			not-null="false"
			length="10"
		/>	
		<many-to-one
			name="targetType"
			column="target_type"
			class="com.app.docmgr.model.Lookup"
			not-null="true"
		>
		</many-to-one>
		<many-to-one
			name="status"
			column="status"
			class="com.app.docmgr.model.Status"
			not-null="true"
		>
		</many-to-one>
		<many-to-one
			name="customer"
			column="customer_id"
			class="com.app.docmgr.model.Customer"
			not-null="true"
		>
		</many-to-one>
		<property
			name="accountTypeCode"
			column="account_type_code"
			type="string"
			not-null="false"
			length="6"
		/>
		
		<!--  
		<property
			name="atmBankCode"
			column="atm_bank_code"
			type="string"
			not-null="false"
			length="10"
		/>
		-->
		
		<many-to-one
			name="bankDetail"
			column="bank_detail"
			class="com.app.docmgr.model.ExternalBank"
			not-null="false"
		>
		</many-to-one>
		
	</class>	
</hibernate-mapping>	
<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.app.docmgr.model">
	<class
		name="IPassportData"
		table="ipassport_data"
	>
		<id
			name="id"
			type="java.lang.Long"
			column="id"
		>
			<generator class="identity"/>
		</id>
		
		<property
			name="loginId"
			column="login_id"
			type="string"
			not-null="true"
			length="30"
		/>
	
		<property
			name="ipassport"
			column="ipassport"
			type="string"
			not-null="false"
			length="70"
		/>
		
		<property
			name="dataClob"
			column="data_clob"
			type="string"
			not-null="false"
			length="4000"
		</property>
		
		
		<property
			name="requestScopeData"
			column="request_scope_data"
			type="string"
			not-null="false"
			length="4000"
		/>
		
		<property
			name="entityVersionData"
			column="entity_version_data"
			type="string"
			not-null="false"
		/>
		
	</class>	
</hibernate-mapping>
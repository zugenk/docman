<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.app.docmgr.model">
	<class
		name="AuthorizationLogHistory"
		table="authorization_log_history"
	>
		<id
			name="id"
			type="java.lang.Long"
			column="id"
		>
			<generator class="sequence">
				<param name="sequence">authorization_log_history_id_s</param>
            </generator>
		</id>

		<property
			name="sessionId"
			column="session_id"
			type="string"
			not-null="true"
			length="50"
		/>
		<many-to-one
			name="doneBy"
			column="done_by"
			class="com.app.docmgr.model.User"
			not-null="true"
		>
		</many-to-one>		
		<property
			name="transactionReferenceNumber"
			column="transaction_reference_number"
			type="string"
			not-null="false"
			length="50"
		/>		
		<!--  
			name="questionId"
			column="question_id"
		-->
		<property 
			name="secureQuestionList"
			column="secure_question_list"
			type="string"
			not-null="false"
			length="30"
		/>
		<!-- 
			name="questionAnswer"
			column="question_answer"
		 -->
		<property 
			name="secureKeyList"
			column="secure_key_list"
			type="string"
			not-null="false"
			length="500"
		/>		
		<many-to-one
			name="status"
			column="status"
			class="com.app.docmgr.model.Status"
			not-null="true"
		>
		</many-to-one>		
		<property
			name="transactionDate"
			column="transaction_date"
			type="java.util.Date"
			not-null="true"
		/>
	</class>	
</hibernate-mapping>
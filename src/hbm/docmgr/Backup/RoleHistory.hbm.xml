<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.app.docmgr.model">
	<class
		name="RoleHistory"
		table="role_history"
	>
		<id
			name="id"
			type="java.lang.Long"
			column="id"
		>
			<generator class="sequence">
				<param name="sequence">role_history_id_seq</param>
            </generator>
		</id>
		<property
			name="historyDate"
			column="history_date"
			type="java.util.Date"
			not-null="true"
		/>
		<property
			name="historyBy"
			column="history_by"
			type="string"
			not-null="true"
			length="30"
		/>
		<property
			name="auditTrailId"
			column="audit_trail_id"
			type="long"
			not-null="true"
		/>
		
		<property
			name="roleId"
			column="role_id"
			type="long"
			not-null="true"
		/>
		<many-to-one
			name="owner"
			column="owner_id"
			class="com.app.docmgr.model.Customer"
			not-null="false"
		>
		</many-to-one>
		<property
			name="name"
			column="name"
			type="string"
			not-null="true"
			length="50"
		/>
		<many-to-one
			name="level"
			column="level_"
			class="com.app.docmgr.model.Lookup"
			not-null="true"
		>
		</many-to-one>
		
		<property
			name="description"
			column="description"
			type="string"
			not-null="false"
			length="100"
		/>
		<many-to-one
			name="status"
			column="status"
			class="com.app.docmgr.model.Status"
			not-null="true"
		>
		</many-to-one>
		<property
			name="createdDate"
			column="created_date"
			type="java.util.Date"
			not-null="true"
		/>
		<property
			name="createdBy"
			column="created_by"
			type="string"
			not-null="true"
			length="30"
		/>
		<property
			name="lastUpdatedDate"
			column="last_updated_date"
			type="java.util.Date"
			not-null="true"
		/>
		<property
			name="lastUpdatedBy"
			column="last_updated_by"
			type="string"
			not-null="true"
			length="30"
		/>

		<set
			name="privileges"
			table="role_history_privilege"
			cascade="all-delete-orphan"
		>
			<key column="role_history_id"/>
			<many-to-many column="privilege_id" class="com.app.docmgr.model.Privilege"/>
		</set>

	</class>	
</hibernate-mapping>
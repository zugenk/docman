<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.app.docmgr.model">
	<class
		name="Case"
		table="case"
	>
		<id
			name="id"
			type="java.lang.Long"
			column="id"
		>
			<generator class="sequence">
				<param name="sequence">case_id_seq</param>
            </generator>
		</id>
		<property
			name="name"
			column="name"
			type="string"
			not-null="false"
			length="50"
		/>
		<property
			name="caseType"
			column="case_type"
				type="string"
			not-null="true"
			length="50"
		/>
		<property
			name="caseNumber"
			column="case_number"
			type="string"
			not-null="false"
			length="30"
		/>
		<property
			name="description"
			column="description"
			type="string"
			not-null="false"
			length="100"
		/>
		<property
			name="currency"
			column="currency"
			type="string"
			not-null="false"
			length="10"
		/>	
		<many-to-one
			name="caseType"
			column="case_type"
			class="com.app.docmgr.model.Lookup"
			not-null="true"
		>
		</many-to-one>
		<many-to-one
			name="status"
			column="status"
			class="com.app.docmgr.model.Status"
			not-null="true"
		>
		</many-to-one>
		<many-to-one
			name="officeUnit"
			column="office_unit_id"
			class="com.app.docmgr.model.officeUnit"
			not-null="true"
		>
		</many-to-one>

		<property
			name="accountTypeCode"
			column="account_type_code"
			type="string"
			not-null="false"
			length="6"
		/>
		
		<many-to-one
			name="caseDetail"
			column="case_detail"
			type="string"
			not-null="false"
			length="4000"
		>
		</many-to-one>
		
	</class>	
</hibernate-mapping>	
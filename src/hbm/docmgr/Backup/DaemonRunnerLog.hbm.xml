<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.app.docmgr.model">
	<class
		name="DaemonRunnerLog"
		table="daemon_runner_log"
	>
		<id
			name="id"
			type="java.lang.Long"
			column="id"
		>
			<generator class="sequence">
				<param name="sequence">daemon_runner_log_id_seq</param>
            </generator>
		</id>

		<property
			name="user"
			column="user_"
			type="string"
			not-null="true"
			length="50"
		/>
		<property
			name="sessionId"
			column="session_id"
			type="string"
			not-null="false"
			length="50"
		/>
		<property
			name="transactionReferenceNumber"
			column="transaction_reference_number"
			type="string"
			not-null="false"
			length="50"
		/>
		<property
			name="operation"
			column="operation"
			type="string"
			not-null="true"
			length="50"
		/>
		<property
			name="message"
			column="message"
			type="text"
			not-null="false"
			length="1000"
		/>
		<many-to-one
			name="status"
			column="status"
			class="com.app.docmgr.model.Status"
			not-null="true"
		>
		</many-to-one>
		
		<property
			name="startDate"
			column="start_date"
			type="java.util.Date"
			not-null="true"
		/>
		<property
			name="finishedDate"
			column="created_date"
			type="java.util.Date"
			not-null="true"
		/>
	</class>
</hibernate-mapping>
